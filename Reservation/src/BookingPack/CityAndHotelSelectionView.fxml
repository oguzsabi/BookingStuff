<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="530.0" prefWidth="921.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="BookingPack.CityAndHotelSelectionController">
   <children>
      <ListView fx:id="cityList" prefHeight="530.0" prefWidth="252.0">
         <padding>
            <Insets left="5.0" />
         </padding>
         <effect>
            <InnerShadow />
         </effect>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></ListView>
      <ListView fx:id="hotelListForAllCities" layoutX="252.0" prefHeight="530.0" prefWidth="252.0">
         <effect>
            <InnerShadow />
         </effect>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></ListView>
      <AnchorPane fx:id="descriptionAndImagePane" layoutX="504.0" prefHeight="272.0" prefWidth="417.0" style="-fx-background-color: #7fb3c6;">
         <children>
            <Label fx:id="cityOrHotelName" prefHeight="53.0" prefWidth="417.0" text="Label">
               <font>
                  <Font size="36.0" />
               </font>
               <padding>
                  <Insets left="5.0" />
               </padding></Label>
            <Text fx:id="cityOrHotelDescription" layoutX="13.0" layoutY="74.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" wrappingWidth="391.0" />
            <Separator layoutY="45.0" prefHeight="16.0" prefWidth="417.0" />
            <Separator layoutY="217.0" prefHeight="16.0" prefWidth="417.0" />
            <Text fx:id="hotelRoomAmountOrCityPopulationText" fontSmoothingType="LCD" layoutX="3.0" layoutY="238.0" strokeType="OUTSIDE" strokeWidth="0.0" text="City Population" underline="true">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font></Text>
            <Text fx:id="yearOfBuiltOrCityAltitudeText" fontSmoothingType="LCD" layoutX="158.0" layoutY="238.0" strokeType="OUTSIDE" strokeWidth="0.0" text="City Altitude(m) " underline="true">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font></Text>
            <Text fx:id="nightlyCostOrAnnualTouristVisitText" fontSmoothingType="LCD" layoutX="298.0" layoutY="238.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Annual Tourist Visit " underline="true">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font></Text>
            <Text fx:id="cityPopulationText" layoutX="24.0" layoutY="253.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
            <Text fx:id="cityAltitudeText" layoutX="184.0" layoutY="254.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
            <Text fx:id="annualTouristVisitText" layoutX="332.0" layoutY="253.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
         </children>
         <effect>
            <InnerShadow />
         </effect></AnchorPane>
      <ImageView fitHeight="259.0" fitWidth="417.0" layoutX="504.0" layoutY="271.0" opacity="0.5" pickOnBounds="true">
         <image>
            <Image url="@Holiday.jpg" />
         </image>
         <effect>
            <InnerShadow />
         </effect></ImageView>
      <Button fx:id="backButton" layoutX="518.0" layoutY="478.0" mnemonicParsing="false" onMouseClicked="#backButtonClicked" prefHeight="25.0" prefWidth="87.0" text="Back">
         <effect>
            <InnerShadow />
         </effect>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <Button fx:id="nextButton" layoutX="830.0" layoutY="478.0" mnemonicParsing="false" onMouseClicked="#nextButtonClicked" prefHeight="25.0" prefWidth="87.0" text="Next">
         <effect>
            <InnerShadow />
         </effect>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <Separator layoutX="504.0" layoutY="264.0" prefHeight="16.0" prefWidth="417.0" />
      <Label fx:id="priceWarning" layoutX="818.0" layoutY="257.0" text="(Prices may vary.)">
         <font>
            <Font size="9.0" />
         </font>
      </Label>
      <Button layoutX="659.0" layoutY="478.0" mnemonicParsing="false" onMouseClicked="#myReservationsButtonClicked" prefHeight="25.0" prefWidth="115.0" text="My reservations">
         <effect>
            <InnerShadow />
         </effect>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
   </children>
</AnchorPane>
